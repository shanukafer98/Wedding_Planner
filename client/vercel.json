{
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/path-needing-different-coop/(.*)",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "unsafe-none"
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
            {
              "source": "/(.*)",
              "headers": [
                {
                  "key": "Cross-Origin-Opener-Policy",
                  "value": "unsafe-none"
                }
              ]
            }
          ]
      }
    ]
  }